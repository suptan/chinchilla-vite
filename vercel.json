{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": ".output/express/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": [".output/express/**"] }
    },
    {
      "src": ".output/vite/dist/**",
      "use": "@vercel/static",
      "config": { "includeFiles": [".output/vite/dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": ".output/express/server.js"
    },
    {
      "src": "/",
      "dest": ".output/vite/dist/index.html"
    },
    {
      "src": "/(.+)",
      "dest": ".output/vite/dist/$1"
    }
  ]
}