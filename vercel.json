{
    "version": 2,
    "env": {
    },
    "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install --frozen-lockfile",
    "buildCommand": "~/.bun/bin/bun run build",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": ".local/bun/server.js"
      },
      {
        "src": "/",
        "dest": ".local/vite/dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": ".local/vite/dist/$1"
      }
    ]
  }